echo "$as_me: error: cannot create directory $dirpart/$fdir" >&2;}
   { (exit 1); exit 1; }; }; }

    # echo "creating $dirpart/$file"
    echo '# dummy' > "$dirpart/$file"
  done
done
 ;;
    default )


# vvvvvvvvvvvvvvvvvvvvvv GASNET_FIX_EXEC_OUTPUT vvvvvvvvvvvvvvvvvvvvvv (L:1)

