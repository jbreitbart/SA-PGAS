  no)  enabled_sgi_ip27=no ;;
  yes) enabled_sgi_ip27=yes ;;
  *)   enabled_sgi_ip27=unknown ;;
esac

# ^^^^^^^^^^^^^^^^^^^^^^ GASNET_IF_ENABLED_WITH_AUTO(arch-sgi-ip27,...) ^^^^^^^^^^^^^^^^^^^^^^ (L:1)



