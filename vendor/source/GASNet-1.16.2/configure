             enabled_sgi_ip27=no
	   fi ;;
    *) # Linux, others?
       enabled_sgi_ip27=no
       ;;
  esac
  echo "$as_me:$LINENO: result: $enabled_sgi_ip27" >&5
echo "${ECHO_T}$enabled_sgi_ip27" >&6
fi
